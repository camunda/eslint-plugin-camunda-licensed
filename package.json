{
  "name": "eslint-plugin-camunda-licensed",
  "version": "1.0.0",
  "description": "Shared lint rules for Camunda licensed OSS projects",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./configs/*.js": "./configs/*.js",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/camunda/eslint-plugin-camunda-licensed.git"
  },
  "scripts": {
    "all": "npm test",
    "test": "npm test -w test"
  },
  "keywords": [
    "eslint",
    "lint",
    "rules",
    "bpmn.io"
  ],
  "author": "Nico Rehwaldt <nico.rehwaldt@camunda.com>",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-license-header": "^0.6.1"
  },
  "devDependencies": {
    "eslint": "^9.11.1"
  },
  "peerDependencies": {
    "eslint": "^9"
  },
  "files": [
    "configs",
    "resources",
    "index.js"
  ],
  "workspaces": [
    "test"
  ]
}
