{
  "name": "test",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "npm run lint && npm run fix",
    "lint": "npm-run-all lint:*",
    "lint:apache": "eslint apache/valid --config apache/eslint.config.js",
    "lint:commercial": "eslint commercial/valid --config commercial/eslint.config.js",
    "lint:mit": "eslint mit/valid --config mit/eslint.config.js",
    "fix": "npm-run-all fix:*",
    "fix:apache": "eslint apache/fix --config apache/eslint.config.js --fix-dry-run",
    "fix:commercial": "eslint commercial/fix --config commercial/eslint.config.js --fix-dry-run",
    "fix:mit": "eslint mit/fix --config mit/eslint.config.js --fix-dry-run"
  },
  "devDependencies": {
    "eslint-plugin-camunda-licensed": "file:..",
    "npm-run-all": "^4.1.5"
  }
}
